{
	"name": "contao/core",
	"description": "Contao Open Source CMS",
	"keywords": ["contao","core"],
	"homepage": "https://contao.org",
	"license": "LGPL-3.0",
	"authors": [
		{
			"name":"Leo Feyer",
			"email":"github@contao.org",
			"homepage":"https://contao.org",
			"role":"Project leader"
		}
	],
	"support": {
		"email":"github@contao.org",
		"issues":"https://github.com/contao/core/issues",
		"irc":"irc://irc.freenode.org/contao",
		"source":"https://github.com/contao/core.git"
	},
	"require": {
		"php": ">=5.3.2",
		"contao-community-alliance/composer-plugin": "~2.0"
	},
	"replace": {
		"contao/calendar-bundle": "self.version",
		"contao/comments-bundle": "self.version",
		"contao/core-bundle": "self.version",
		"contao/faq-bundle": "self.version",
		"contao/listing-bundle": "self.version",
		"contao/news-bundle": "self.version",
		"contao/newsletter-bundle": "self.version"
	},
	"autoload": {
		"classmap": [
			"system/modules/backend",
			"system/modules/calendar",
			"system/modules/comments",
			"system/modules/faq",
			"system/modules/frontend",
			"system/modules/listing",
			"system/modules/news",
			"system/modules/newsletter",
			"system/modules/registration",
			"system/modules/rep_base",
			"system/modules/rep_client",
			"system/modules/rss_reader",
			"system/modules/tasks",
			"system/modules/tpl_editor",
			"system/libraries"
		]
	}
}
